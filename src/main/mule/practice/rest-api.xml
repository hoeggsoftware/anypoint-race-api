---
payload ++ {
	start: now()
}]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<os:store doc:name="racerState" doc:id="640e8f9f-c6bc-4097-923d-990354752b3a" key='#["racerState.$(vars.racerId)"]' objectStore="practiceObjectStore"/>
