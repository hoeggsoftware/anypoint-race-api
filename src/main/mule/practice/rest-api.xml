	<flow name="practice_restapi_check" doc:id="930f73b2-74e1-4cb6-8a3d-474f242834c5" >
		<logger level="INFO" doc:name="Logger" doc:id="99b8b3d3-d63a-4900-a3b4-0d974aef0d4f" />
	</flow>
</mule>
